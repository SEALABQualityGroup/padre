operation Artifact F_maxHwUtil(th_maxHwUtil : Real, operator : String) : Boolean {
	var criticalDev = self.getCriticalDeploymentDevice();//.println("\tCRITICAL DEV: ");
	if(criticalDev.isDefined()) {
		//criticalDev.getName().println("CRITICAL DEV FOR Artifact " + self.getName() + ": ");
		var utilization = criticalDev.getMaxUtilization();
		if( utilization.isDefined()) {
			if(operator.equals("geq")) {
				if(utilization.asReal() >= th_maxHwUtil )
					return true;
			} else if(operator.equals("leq")) {
				if(utilization.asReal() <= th_maxHwUtil )
					return true;
			} else
				return false;
		} else
			return false;
	}
	return false;
}



