<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">	<xs:complexType name="T4_sig">		<xs:sequence>			<xs:element name="atom" maxOccurs="unbounded">				<xs:complexType>					<xs:attribute name="name" use="required" type="xs:string"/>				</xs:complexType>			</xs:element>		</xs:sequence>		<xs:attribute name="name" use="required" type="xs:string"/>		<xs:attribute name="isOne" type="xs:boolean"/>		<xs:attribute name="isBuiltin" type="xs:boolean"/>		<xs:attribute name="isAbstract" type="xs:boolean"/>		<xs:attribute name="extends" use="required" type="xs:string"/>	</xs:complexType>	<xs:complexType name="T4_field">		<xs:sequence>			<xs:element name="type">				<xs:complexType>					<xs:sequence>						<xs:element name="sig" maxOccurs="unbounded">							<xs:complexType>								<xs:attribute name="name" use="required" type="xs:string"/>							</xs:complexType>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>			<xs:element name="tuple" maxOccurs="unbounded">				<xs:complexType>					<xs:sequence>						<xs:element name="atom" maxOccurs="unbounded">							<xs:complexType>								<xs:attribute name="name" use="required" type="xs:string"/>							</xs:complexType>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>		</xs:sequence>		<xs:attribute name="name" use="required" type="xs:string"/>	</xs:complexType>	<xs:element name="TestSuite">		<xs:complexType>			<xs:sequence>				<xs:element name="TestCase">					<xs:complexType>						<xs:sequence>							<xs:element name="Function">								<xs:complexType>									<xs:attribute name="returnValue" use="required" type="xs:boolean"/>									<xs:attribute name="precondition" use="required" type="xs:byte"/>									<xs:attribute name="name" use="required" type="xs:string"/>								</xs:complexType>							</xs:element>							<xs:element name="instance">								<xs:complexType>									<xs:sequence>										<xs:element name="sig" maxOccurs="unbounded" type="T4_sig"/>										<xs:element name="field" maxOccurs="unbounded" type="T4_field"/>										<xs:element name="sig" type="T4_sig"/>										<xs:element name="field" type="T4_field"/>										<xs:element name="sig" type="T4_sig"/>										<xs:element name="field" type="T4_field"/>										<xs:element name="sig" type="T4_sig"/>										<xs:element name="field" type="T4_field"/>										<xs:element name="sig" type="T4_sig"/>										<xs:element name="field" maxOccurs="unbounded" type="T4_field"/>										<xs:element name="sig" type="T4_sig"/>										<xs:element name="field" maxOccurs="unbounded" type="T4_field"/>										<xs:element name="set" maxOccurs="unbounded">											<xs:complexType>												<xs:sequence>													<xs:element name="atom">														<xs:complexType>															<xs:attribute name="name" use="required" type="xs:string"/>														</xs:complexType>													</xs:element>												</xs:sequence>												<xs:attribute name="type" use="required" type="xs:string"/>												<xs:attribute name="name" use="required" type="xs:string"/>											</xs:complexType>										</xs:element>									</xs:sequence>									<xs:attribute name="filename" use="required" type="xs:string"/>									<xs:attribute name="command" use="required" type="xs:string"/>									<xs:attribute name="bitwidth" use="required" type="xs:byte"/>								</xs:complexType>							</xs:element>						</xs:sequence>						<xs:attribute name="SampleSet" use="required" type="xs:string"/>						<xs:attribute name="Region" use="required" type="xs:byte"/>						<xs:attribute name="PostStateInclusion" use="required" type="xs:string"/>						<xs:attribute name="Notes" use="required" type="xs:string"/>						<xs:attribute name="NoOfTrials" use="required" type="xs:byte"/>						<xs:attribute name="No" use="required" type="xs:string"/>					</xs:complexType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>